	if (a <= b) {
		printf("a <= b");
	} else {
		printf("not a <= b");
	}
	if (a == b) {
		printf("a == b");
	} else {
		printf("not a == b");
	}
	if (a != b) {
		printf("a != b");
	} else {
		printf("not a != b");
	}
	if (a) {
		printf("a");
	} else {
		printf("not a");
	}
	if (b) {
		printf("b");
	} else {
		printf("not b");
	}

	if (b > a) {
		printf("b > a");
	} else {
		printf("not b > a");
	}
	if (b >= a) {
		printf("b >= a");
	} else {
		printf("not b >= a");
	}
	if (b < a) {
		printf("b < a");
	} else {
		printf("not b < a");
	}
	if (b <= a) {
		printf("b <= a");
	} else {
		printf("not b <= a");
	}
	if (b == a) {
		printf("b == a");
	} else {
		printf("not b == a");
	}
	if (b != a) {
		printf("b != a");
	} else {
		printf("not b != a");
	}
	if (b) {
		printf("b");
	} else {
		printf("not b");
	}
	if (a) {
		printf("a");
	} else {
		printf("not a");
	}

	if (3 > b) {
		printf("3 > b");
	} else {
		printf("not 3 > b");
	}
	if (3 >= b) {
		printf("3 >= b");
	} else {
		printf("not 3 >= b");
	}
	if (3 < b) {
		printf("3 < b");
	} else {
		printf("not 3 < b");
	}
	if (3 <= b) {
		printf("3 <= b");
	} else {
		printf("not 3 <= b");
	}
	if (3 == b) {
		printf("3 == b");
	} else {
		printf("not 3 == b");
	}
	if (3 != b) {
		printf("3 != b");
	} else {
		printf("not 3 != b");
	}
	if (3) {
		printf("3");
	} else {
		printf("not 3");
	}
	if (b) {
		printf("b");
	} else {
		printf("not b");
	}
}

void main()
{
	int a, b, c;
	while (1) {
		scanf(a);
		scanf(b);
		scanf(c);
		if (a == 0) {
			if (b == 0) {
				if (c == 0) {
					test_zero();
					return;
				}
			}
		}
		compare(a, b, c);
		compare_to_num(a, b, c);
	}
	return;
}
