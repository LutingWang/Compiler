		printf("a == a");
	} else {
		printf("not a == a");
	}
	if (a != a) {
		printf("a != a");
	} else {
		printf("not a != a");
	}
	if (a) {
		printf("a");
	} else {
		printf("not a");
	}

	if (a > b) {
		printf("a > b");
	} else {
		printf("not a > b");
	}
	if (a >= b) {
		printf("a >= b");
	} else {
		printf("not a >= b");
	}
	if (a < b) {
		printf("a < b");
	} else {
		printf("not a < b");
